<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.viaagnolettisrl.hibernate.NonWorkingDay" table="nonWorkingDays">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property column="begins" name="begins" type="timestamp" />
		<property column="ends" name="ends" type="timestamp" />
	</class>
</hibernate-mapping>