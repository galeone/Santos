<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.viaagnolettisrl.hibernate.AssignedJobOrder"
		table="assignedJobOrders">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property column="starts" name="start" type="timestamp" />
		<property column="ends" name="end" type="timestamp" />
		<many-to-one name="Machine" class="com.viaagnolettisrl.hibernate.Machine"
			column="idMachine" unique="false" not-null="true" lazy="false" />
		<many-to-one name="JobOrder" class="com.viaagnolettisrl.hibernate.JobOrder"
			column="idJobOrder" unique="false" not-null="true" lazy="false" />
	</class>
</hibernate-mapping>