<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!--
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 --><!DOCTYPE hibernate-mapping SYSTEM "/home/paolo/db-gen/src/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-access="property" default-cascade="none" default-lazy="true">
<class dynamic-insert="false" dynamic-update="false" mutable="true" name="com.viaagnolettisrl.hibernate.Machine" optimistic-lock="version" polymorphism="implicit" select-before-update="false" table="machines">
	<id column="id" name="id">
		<generator class="increment"/>
	</id>
	<set embed-xml="true" inverse="true" mutable="true" name="joborders" optimistic-lock="true" sort="unsorted" table="AssignedJobOrders">
<key column="idMachine" on-delete="noaction"/>
<many-to-many class="com.viaagnolettisrl.hibernate.JobOrder" column="idJobOrder" embed-xml="true" not-found="exception" unique="false"/>
</set>
<property column="NAME" generated="never" lazy="false" length="50" name="name" optimistic-lock="true" type="string" unique="false"/>
	<property column="COLOR" generated="never" lazy="false" length="50" name="color" optimistic-lock="true" type="string" unique="false"/>
</class>
</hibernate-mapping>
